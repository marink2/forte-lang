{
  "name": "forte-lang",
  "displayName": "forte-lang",
  "description": "Language support for Forte input.",
  "repository": {
    "type": "git",
    "url": "https://github.com/marink2/forte-lang.git"
  },
  "author": "marink2",
  "publisher": "marink2",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dat",
        "aliases": [
          "Forte Input",
          "dat"
        ],
        "extensions": [
          ".dat"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dat",
        "scopeName": "source.python.dat",
        "path": "./syntaxes/dat.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "dat",
        "path": "./snippets/dat.code-snippets"
      }
    ]
  }
}