{
  "name": "forte-lang",
  "displayName": "forte-lang",
  "description": "Language support for Forte input.",
  "main": "./src/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/marink2/forte-lang.git"
  },
  "author": "marink2",
  "publisher": "marink2",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dat",
        "aliases": [
          "Forte Input",
          "dat"
        ],
        "extensions": [
          ".dat"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dat",
        "scopeName": "source.python.dat",
        "path": "./syntaxes/dat.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "dat",
        "path": "./snippets/dat.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "extension.plotOrbitals",
        "title": "Plot Orbitals"
      },
      {
        "command": "extension.plotMolecule",
        "title": "Plot Molecule"
      },
      {
        "command": "extension.runPsi4Bash",
        "title": "Run psi4",
        "icon": {
          "light": "assets/RunPsi4IconLight.svg",
          "dark": "assets/RunPsi4IconDark.svg"
        }
      }
    ],
    "keybindings": [
      {
        "command": "extension.plotOrbitals",
        "key": "ctrl+o",
        "mac": "cmd+o"
      },
      {
        "command": "extension.plotMolecule",
        "key": "ctrl+m",
        "mac": "cmd+m"
      }
    ],
    "menus": {
      "editor/title/run": [
        {
          "command": "extension.runPsi4Bash",
          "when": "editorLangId == dat",
          "group": "navigation@0"
        }
      ]
    }
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "three": "^0.169.0"
  }
}